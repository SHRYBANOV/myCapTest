{  "name": "vacantions",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/attachments": "^3.7.0",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-vacantion1": "cds watch --open vacantion1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project1": "cds watch --open project1/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds" : { "requires" : {
      "db": {
        "kind": "sqlite",
        "credentials" : { "url" : "db.sqlite"},
        "schema_evolution" : "auto"
      }},
  "auth" : {
    "kind" : "mosked",
    "users" : {
      "alice" : {
        "password" : "123",
        "roles" : ["admin"]
      }
    }
  }
}, 
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/project1"

  ],
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
